<script type="text/javascript">
var c = 0;
var d = 'a';
function taby(event)
{
  var a = document.getElementById("ids1").value+','+document.getElementById("ids3").value+','+document.getElementById("ids2").value;
  var b = a.split(',');
  if(event.keyCode == 9)
  {
	if(d=='f')
	{
		d='a';
		if(c==(b.length-1))
		{
			c=0;
		}
		else
		{
			c=c+1;
		}
	}
	else
	{
		d=String.fromCharCode(d.charCodeAt(0)+1);
	}
	if(document.getElementById('grp'+b[c]+d) != null)
	{
		$('#grp'+b[c]+'a').focus();
		document.getElementById('grp'+b[c]+d).checked = true;
	}
	else
	{
		taby(event);
	}
  }
}
if ($.browser.mozilla) {
    $(document).keypress (taby);
} else {
    $(document).keydown (taby);
}
</script>
